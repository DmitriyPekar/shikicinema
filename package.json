{
  "name": "shikicinema",
  "version": "0.6.2",
  "description": "Returns video player on Shikimori",
  "main": "src/shikicinema.js",
  "scripts": {
    "bundle": "cd src/ui && ng b && cd ../.. && webpack --mode development",
    "release": "cd src/ui && ng b --prod && cd ../.. && webpack --mode production",
    "watch": "webpack --mode development && cd src/ui && ng b --watch --delete-output-path=false",
    "test": "echo 'no tests specified'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Smarthard/shikicinema.git"
  },
