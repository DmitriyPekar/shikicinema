  "bugs": {
    "url": "https://github.com/Smarthard/shikicinema/issues"
  },
  "homepage": "https://github.com/Smarthard/shikicinema",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
