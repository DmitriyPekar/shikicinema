{
  "manifest_version": 2,
  "name": "shikicinema",
  "version": "0.0.1",

  "description": "Returns video player on Shikimori",

  "content_scripts": [
    {
      "matches": [
        "*://shikimori.org/animes/*"
      ],
      "js": [
        "shikicinema.js"
      ],
      "css": [
        "shikicinema.css"
      ]
    }
  ]
}
